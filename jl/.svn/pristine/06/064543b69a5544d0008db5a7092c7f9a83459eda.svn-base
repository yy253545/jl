.posFix {
	position: fixed;
}

.posAbs {
	position: absolute;
}

.posRel {
	position: relative;
}

.wh {
	width: 100%;
	height: 100%;
}

.cenMid {
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.cen {
	left: 50%;
	transform: translateX(-50%);
}

.mid {
	top: 50%;
	transform: translateY(-50%);
}

#contactBox {
	width: 70px;
	height: 295px;
	bottom: 5%;
	right: 75px;
	line-height: 0px;
	z-index: 10;
	display: none;
}

.contactBox {
	background: url("../contactImg/bunner_03.png") no-repeat;
	line-height: 0px;
	background-size: cover;
	-webkit-background-size: 100%;
	-o-background-size: 100%;
	background-position: center 0;
	top: 0px;
	left: 0px;
}

.contactItemBox {
	width: 70px;
	height: 70px;
	display: block;
}

.contactItemBox .icon {
	width: 40px;
	height: 40px;
}

.contactItemBox .ewmIcon {
	width: 90px;
	height: 90px;
	left: -135%;
}

.lineIcon {
	width: 70px;
	height: 1px;
}
