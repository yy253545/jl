		<script>
			window.onscroll = function() {
				var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
				var mainHeight = document.body.offsetHeight;
				var per = scrollTop / mainHeight;
				// console.log(per);
				if (per > 0.20) {
					$("#contactBox").slideDown(1000);
				} else if (per < 0.19) {
					$("#contactBox").slideUp(1000);
				}
			}
			//关闭侧栏
			$('.x_img').click(function() {
				$('#right').hide();
			})
			//当点击跳转链接后，回到页面顶部位置
			$(".backTopBox").click(function() {
				if ($('html').scrollTop()) {
					$('html').animate({
						scrollTop: 0
					}, 1000);
					return false;
				}
				$('body').animate({
					scrollTop: 0
				}, 1000);
				return false;
			});

			$(".contactItemBox").mouseover(function() {
				$(this).find(".ewmIcon").show();
				$(this).siblings().find(".ewmIcon").hide();
			});
			$(".ewmIcon").mouseleave(function() {
				$(this).hide();
			});

			// 底部轮播图的显示效果JS
			var index = 0; //默认是显示第一张;
			var swiperInterval;
			$(".gif_bimg_i a").eq(index).show().siblings().hide();
			swiperInterval = setInterval(function() {
				index++;
				if (index > 3) { //到最后一张了显示第一张
					index = 0;
				}
				$(".gif_bimg_i a").eq(index).show().siblings().hide();
			}, 5000);

			$(".gif_simg_g img").mouseover(function() { //鼠标进入
				// console.log($(this).index());
				index = $(this).index();
				clearInterval(swiperInterval);
				$(".gif_bimg_i a").eq(index).show().siblings().hide();
			});

			$(".gif_simg_g img").mouseout(function() { //鼠标移出
				// console.log($(this).index());
				index = $(this).index();
				swiperInterval = setInterval(function() {
					index++;
					if (index > 3) { //到最后一张了显示第一张
						index = 0;
					}
					$(".gif_bimg_i a").eq(index).show().siblings().hide();
				}, 5000);
			});

			$("#gif_simg_bl").mouseover(function() {
				clearInterval(swiperInterval);
			});

			$("#gif_simg_br").mouseover(function() {
				clearInterval(swiperInterval);
			});

			$("#gif_simg_bl").on("click", function() { //点击上一个
				index--;
				if (index < 0) { //到第一张了,显示最后一张;
					index = 3;
				};
				$(".gif_bimg_i a").eq(index).show().siblings().hide();
			});

			$("#gif_simg_br").on("click", function() { //点击上一个
				index++;
				if (index > 3) { //到第一张了,显示最后一张;
					index = 0;
				};
				$(".gif_bimg_i a").eq(index).show().siblings().hide();
