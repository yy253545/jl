@-webkit-keyframes aniArrowA {
    0% {-webkit-transform:translateY(-.15rem)}
    100% {-webkit-transform:translateY(0.15rem)}
}
@-moz-keyframes aniArrowA {
    0% {
        -moz-transform:translateY(-.15rem)
    }
    100% {
        -moz-transform:translateY(0.15rem)
    }
}
@-o-keyframes aniArrowA {
    0% {
        -o-transform:translateY(-.15rem)
    }
    100% {
        -o-transform:translateY(0.15rem)
    }
}
@-ms-keyframes aniArrowA {
    0% {
        -ms-transform:translateY(-.15rem)
    }
    100% {
        -ms-transform:translateY(0.15rem)
    }
}
@keyframes aniArrowA {
    0% {
        transform:translateY(-.15rem)
    }
    100% {
        transform:translateY(0.15rem)
    }
}

.menu-btn-off.menu-btn-active i:nth-child(2n+1) {
    -webkit-transform:translate(0,.2rem);
    -moz-transform:translate(0,.2rem);
    -ms-transform:translate(0,.2rem);
    transform:translate(0,.2rem);
    opacity:0
}
.menu-btn-off.menu-btn-active i:nth-child(2n+2) b:nth-child(2n+1) {
    -webkit-transform:rotate(45deg);
    -moz-transform:rotate(45deg);
    -ms-transform:rotate(45deg);
    transform:rotate(45deg)
}
.menu-btn-off.menu-btn-active i:nth-child(2n+2) b:nth-child(2n+2) {
    -webkit-transform:rotate(-45deg);
    -moz-transform:rotate(-45deg);
    -ms-transform:rotate(-45deg);
    transform:rotate(-45deg)
}
.menu-btn-off.menu-btn-active i:nth-child(2n+3) {
    -webkit-transform:translate(0,-.2rem);
    -moz-transform:translate(0,-.2rem);
    -ms-transform:translate(0,-.2rem);
    transform:translate(0,-.2rem);
    opacity:0
}