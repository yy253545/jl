*{margin:0;padding:0;box-sizing:border-box; -webkit-tap-highlight-color:rgba(0, 0, 0, 0);}
html, body{font-family:Helvetica, sans-serif; width: 100%; }
a{outline:0; text-decoration:none;}
ul,li{list-style:none;}
select{outline:none;}
/*input{-webkit-appearance:none;}*/
html {-ms-touch-action: none;}
body {font-size:1rem; color:#a57f5b; font-family:Microsoft YaHei, Arial, Helvetica, sans-serif; background-color:#ccc; max-width:640px; margin:0 auto;}
a {color:#696969;}

/* 媒体查询 */
html{font-size:40px} 
@media screen and (min-width:320px){html{font-size:20px}}
@media screen and (min-width:360px){html{font-size:22.5px;}}
@media screen and (min-width:375px){html{font-size:23.44px}}
