	color: #666;
}

.flex {
	display: -webkit-box;
	display: -moz-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
}

.flexAro {
	justify-content: space-around;
	align-items: center;
}

.flexBet {
	justify-content: space-between;
	align-items: center;
}

.flexBetEnd {
	justify-content: space-between;
	align-items: flex-end;
}

.flexSta {
	justify-content: flex-start;
	align-items: center;
}

.flexEnd {
	justify-content: flex-end;
	align-items: center;
}

.flexCen {
	justify-content: center;
	align-items: center;
}

.flexAroCol {
	flex-direction: column;
	justify-content: space-around;
	align-items: flex-start;
}

.flexBetCol {
	flex-direction: column;
	justify-content: space-between;
	align-items: center;
}

.flexAroColCen {
	flex-direction: column;
	justify-content: space-around;
	align-items: center;
}

.flexEndColCen {
	flex-direction: column;
	justify-content: flex-end;
	align-items: center;
}

.posRel {
