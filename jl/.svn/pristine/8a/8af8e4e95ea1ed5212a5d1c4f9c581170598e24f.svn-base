				var story_bunner_height = $(".story_bunner ul li").eq(1).height() - 100;
				$(".story_bunner ul li").height(story_bunner_height + 'px');
				$(".story_bunner ul .active").width(story_bunner_height + 'px');
				$('.story_bunner ul li').mouseover(function() {
					$(this).addClass('active');
					$(this).siblings().removeClass('active');
					$(".story_bunner ul li").height(story_bunner_height + 'px');
					$(".story_bunner ul li").width(9 + '%');
					$(".story_bunner ul .active").width(story_bunner_height + 'px');
				})
			}
			$(function() {
				// 大图显示
				$(".video_img li .small").click(function() {
					$(this).siblings("img").show();
				})
				$(".video_img li span").click(function() {
					var img = $(this).siblings("img").attr("data-img");
					$(".img-views img").attr({
						"src": img
					});
					$(".img-views").slideDown(400).show();
				});
				$(".img-views span").on("click", function() {
					$("#game-img").slideUp(400);
				})

				//BUNNER轮播图
				var swiper = new Swiper('.swiper-container', {
					pagination: '.swiper-pagination',
					paginationClickable: true,
					// 自动播放时间
					autoplay: 2000,
					// 播放的速度
					speed: 1000
				});

				//header li切换
				$('.header_right .nav li').click(function() {
					$(this).addClass('active');
					$(this).siblings().removeClass('active');
				})

				//职业介绍--职业选择
				let role_tab_index = 1;
				$('.chose_role').hide().eq(role_tab_index).show();
				$('.role_tab ul li').click(function() {
					$('.role_1').css('left', '-35%');
					$('.role_2').css('right', '-35%');
					role_tab_index = $(this).index();
					$('.chose_role').hide().eq(role_tab_index).show();
					$('.role_1').show().animate({
						left: "0%"
					});
					$('.role_2').show().animate({
						right: "0%"
					});
					var _img = $(this).find('img');
					var _index = $(this).index() + 1;
					$('.role_tab ul li').each(function(index) {
						$(this).find('img').addClass('head_off').attr('src', './images_pc/' + (index + 1) +
							'-head.png')
					})
					_img.attr('src', './images_pc/' + _index + '-head-on.png').removeClass('head_off')
				})

				//影音专区--翻页
				tab();
				function tab() {
					var $obj = $('.video_img');
					var len = $obj.length;
					var i = 0;
					$(".button-left").click(function() {
						i++;
						if (i == len) {
							i = 0;
						}
						$obj.stop(true, true).hide().eq(i).fadeIn(600);
						return false;
					});
					$(".button-right").click(function() {
						i--;
						if (i == -1) {
							i = len - 1;
						}
						$obj.stop(true, true).hide().eq(i).fadeIn(600);
						return false;
					});
				}

				//关闭侧栏
				$('.x_img').click(function() {
					$('#right').hide();
				})
				$('.btn_img').click(function() {
					$('#right').show();
				})
				//bunner_list新闻列表切换
				$(".news_title ul li").on("mouseover", function() {
					$(".news_content ul").hide().eq($(this).index()).show();
					$(this).addClass('active');
					$(this).siblings().removeClass('active');
				});
			})
		</script>
