		<script>
			//获取滚动条高度;
			window.onscroll = function(e) {
				var e = e || window.event;
				var scrolltop = document.documentElement.scrollTop || document.body.scrollTop;
				var indexHomeBoxHeight = $("#bunner").height();
				var indexNewsBoxHeight = $("#newsBox").height();
				var indexStoryBoxHeight = $("#story").height();
				var indexRoleBoxHeight = $("#role").height();
				var indexTeseBoxHeight = $("#tese").height();
				var indexVideoBoxHeight = $("#video").height();
				var indexContactBoxHeight = $("#contact").height();
				if (scrolltop > indexHomeBoxHeight * 0.6) {
					$("#newsBox .news_box .swiperBox").addClass("swiper_bounceInRight");
					$("#newsBox .news_box .newsListBox").addClass("newsListBox_bounceInLeft");
				};

				if (scrolltop > (indexHomeBoxHeight + indexNewsBoxHeight) * 0.6) {
					$("#story .story_bunner ul li").eq(0).addClass('story_bunner_bounceInTop');
					setTimeout(function() {
						$("#story .story_bunner ul li").eq(1).addClass('story_bunner_bounceInTop');
					}, 200)
					setTimeout(function() {
						$("#story .story_bunner ul li").eq(2).addClass('story_bunner_bounceInTop');
					}, 400)
					setTimeout(function() {
						$("#story .story_bunner ul li").eq(3).addClass('story_bunner_bounceInTop');
					}, 600)
					setTimeout(function() {
						$("#story .story_bunner ul li").eq(4).addClass('story_bunner_bounceInTop');
					}, 800)
				};
				if (scrolltop > (indexHomeBoxHeight + indexNewsBoxHeight + indexStoryBoxHeight + indexRoleBoxHeight +
						indexTeseBoxHeight + indexVideoBoxHeight) * 0.85) {
					$("#video .video_img li").eq(0).addClass('video_img_bounceInLeft');
					setTimeout(function() {
						$("#video .video_img li").eq(1).addClass('video_img_bounceInLeft');
					}, 200)
					setTimeout(function() {
						$("#video .video_img li").eq(2).addClass('video_img_bounceInLeft');
					}, 400)
					setTimeout(function() {
						$("#video .video_img li").eq(3).addClass('video_img_bounceInLeft');
					}, 600)
					setTimeout(function() {
						$("#video .video_img li").eq(4).addClass('video_img_bounceInLeft');
					}, 800)
					setTimeout(function() {
						$("#video .video_img li").eq(5).addClass('video_img_bounceInLeft');
					}, 1000)
					setTimeout(function() {
						$("#video .video_img li").eq(6).addClass('video_img_bounceInLeft');
					}, 1200)
					setTimeout(function() {
						$("#video .video_img li").eq(7).addClass('video_img_bounceInLeft');
					}, 1400)
				};
			};

			$(function() {
				//首屏添加动效
				setTimeout(function() {
					$(".center_box .sloganImg").addClass("sloganImg_bounceInRight");
					$(".center_box .anniuIcon").addClass("anniuIcon_bounceInLeft");
				}, 1000);
				setTimeout(function() {
					$("#rightBox").addClass("rightBox_bounceInTop");
				}, 1500);
				//添加的动画
				$(".xsLbIcon").addClass("xsLbIconAnimation");
				//点击跳转到新闻页面
				$(".moreNews").on("click", function() {
					window.location.href = './news/index.html';
				})
				//游戏特色方法
				$('.content-part.part-tese').addClass('show');
				$('.gallery_container').gallery_slider({
					imgNum: 5
				});
				//新闻的轮播图
				var newsSwiperIndex = 0;
				var autoPlay = function() {
					$(".swiperIconBox li").eq(newsSwiperIndex).addClass("active");
					$(".swiperIconBox li").eq(newsSwiperIndex).siblings().removeClass("active");
					$(".swiperItemBox").eq(newsSwiperIndex).addClass("activeItem");
					$(".swiperItemBox").eq(newsSwiperIndex).siblings().removeClass("activeItem");
				}
				autoPlay();
				var swiperScroll = setInterval(function() {
					newsSwiperIndex++;
					if (newsSwiperIndex > 3) {
						newsSwiperIndex = 0;
					}
					autoPlay();
				}, 2000);
				$(".swiperIconBox li").on("click", function() {
					clearInterval(swiperScroll);
					newsSwiperIndex = $(this).index();
					autoPlay();
				});

				$('.swiperBox').mouseover(function() {
					clearInterval(swiperScroll);
				});
				$(".swiperBox").mouseleave(function() {
					swiperScroll = setInterval(function() {
						newsSwiperIndex++;
						if (newsSwiperIndex > 3) {
							newsSwiperIndex = 0;
						}
						autoPlay();
					}, 2000);
				});
				
				//礼包点击弹窗
				$(".xsLbIcon").on("click",function(){
					$('#maskBox').show();
					$('.dialogIcon').show();
					$("#maskBox").on("click",function(){
						$('#maskBox').hide();
						$('.dialogIcon').hide();
					})
				})

				//新闻列表模块的切换
				$('.newsTabBox ul li').hover(function() {
					$(this).addClass("active");
					$(this).siblings().removeClass("active");
					var index = $(this).index();
					$(".new").eq(index).show();
					$(".new").eq(index).siblings().hide();
				})

				// 弹出登录框
				function show() {
					closeAll();
					$(".windowblack").height($("body").height()).show();
					$(".window").show();
				}

				function closeAll() {
					$(".windowblack,.window").hide();
				}

				//打开预约窗口
				$('.block_img .btn-b,.download').click(function() {
					$('#get').show();
				})
				//关闭预约窗口
				$('.get_x,.close').click(function() {
					$('#get').hide();
				})

				//视频弹出one
				var $video = $("#game_video video");
				$(".story_bunner ul li img ,.play").click(function() {
					var videoURL = $(this).attr("data-videoURL");
					if (typeof(videoURL) != "undefined") {
						$video.attr("src", videoURL).parents("#game_video").slideDown(400).css("display", "table"); //更新url
						$("#game_video video").attr("autoplay", "true"); //直接播放
					}
					return false
				})
				$video.next("b").on("click", function() {
					$video.get(0).pause();
					$video.parents("#game_video").slideUp(400);
				})

				//关闭右侧弹窗
				$(".leaverIcon").on("click", function() {
					$("#rightBox").animate({
						right: "-8.5%"
					}, 1000);
					$("#rightBox").find(".outIcon").show();
					$("#rightBox").find(".leaverIcon").hide();
				})

				//打开右侧弹窗
				$(".outIcon").on("click", function() {
					$("#rightBox").animate({
						right: "0%"
					}, 1000);
					$("#rightBox").find(".outIcon").hide();
					$("#rightBox").find(".leaverIcon").show();
				})

				//分享按钮
				$('.share').mouseover(function() {
					$('#share_box').slideDown('1000');
					$('.share').hide('1000');
				});
				$('#share_box ul li').click(function() {
					$('#share_box').slideUp('1000');
					$('.share').show('1000');
				})

				//鼠标移入微信分享按钮
				$('.wechat_mouseover').mouseover(function() {
					$('.wechat').show();
				})
				$('.wechat').mouseout(function() {
					$('#share_box').slideUp('1000');
					$('.wechat').hide();
					$('.share').show('1000');
				})

				//点击除分享区域外的地方，隐藏分享区域
				$(document).click(function() {
					$('#share_box').slideUp('1000');
					$('.share').show('1000');
					$('.wechat').hide();
				});
				$("#share_box").click(function(event) {
					event.stopPropagation();
				});
			})
		</script>

