body,input,textarea,select{font-family:"Microsoft YaHei", arial; font-size:12px; color:#5f5f5f; }
body,h1,h2,h3,h4,h5,h6,p,ul,ol,dl,dd,input,li{margin:0; padding:0;}
ul,ol {padding-left:0; list-style-type:none;}
img {border:0;}
body {background:#000102; line-height:20px; overflow-x:hidden;}
input[type='text'],input[type='password'],textarea{ outline-style:none; -webkit-appearance:none;}
a { outline:none; *blr:expression(this.onFocus=this.blur());}
.clear {clear:both}

.st {font-weight:bold;}
.f14 {font-size:14px;}
.hide {display:none;}

a {text-decoration:none;}
a:link,a:visited {color:#5f5f5f; text-decoration:none;}
a:hover,a:active,a:focus{color:#ff662a;text-decoration:none;}

